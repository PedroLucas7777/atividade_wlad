programa {
  inclua biblioteca Matematica --> mat

  funcao inicio() {
    real resultado, base, expoente

    escreva(" Qual é o valor da base?: ")
    leia(base)
    escreva(" Qual é o valor do expoente?: ")
    leia(expoente)

    resultado = mat.potencia(base,expoente)
    escreva(" O resultado é: ",resultado)
  }
}
// ATIVIDADE: base 6 , expoente -4
